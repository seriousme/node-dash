{
  "name": "node-dash",
  "version": "0.0.6",
  "description": "Serverless system",
  "main": "index.js",
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "pretest": "eslint *.js",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 10000 --exit",
    "startdb": "pouchdb-server -d pouchdb",
