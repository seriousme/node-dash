    "vm2": "^3.8.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
